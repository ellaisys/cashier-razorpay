{
    "name": "ellaisys/cashier-razorpay",
    "description": "Laravel Cashier package for Razorpay payment gateway in India. The package uses Razorpay SDK for PHP",
    "keywords": ["php", "laravel", "razorpay", "cashier", "ellaisys", "billing", "payment"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/ellaisys/cashier-razorpay",
    "authors": [
        {
            "name": "EllaiSys",
            "email": "support@ellaisys.com",
            "role": "owner",
            "homepage": "https://ellaisys.github.io/"
        }
    ],
    "support": {
        "email": "support@ellaisys.com"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0|^8.0",
        "razorpay/razorpay": ">=2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
          "Ellaisys\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ellaisys\\Cashier\\CashierServiceProvider"
            ],
            "aliases": {
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
